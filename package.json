{
  "name": "api_epi-control",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
    "start": "node src/server.js",
    "dev:load-envs": "dotenv -e .env.development",
    "seed:run": "prisma db seed",
    "prisma:generate:postgre": "npx prisma generate --schema=./prisma/postgresql/schema.prisma ",
    "prisma:generate:mongo": "npx prisma generate --schema=./prisma/mongodb/schema.prisma",
    "prisma:migrate:dev": "npx prisma migrate dev --schema=./prisma/postgresql/schema.prisma",
    "prisma:run:dev": "npm run prisma:generate:postgre && npm run prisma:generate:mongo && npm run prisma:migrate:dev && npm run seed:run",
    "project:setup": "docker compose up -d && npm run prisma:run:dev",
    "build": "tsc",
    "test": "cross-env NODE_ENV=development jest",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@prisma/client": "^4.16.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^8.9.0",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "prisma": "^4.16.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.25.2",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "nodemon": "^2.0.2",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
